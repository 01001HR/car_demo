<?xml version="1.0" ?>
<%
  # 2016 Toyota Prius with rigid suspension, front steering
  # Consists of chassis with 2 steerable and 2 non-steerable wheels
  # SI units (length in meters)

  # gravity constant [m/s^2]
  grav = 9.81

  # Wheel parameters
  wheel_radius = 0.31265
  wheel_width = 0.215
  wheel_visual_offset = 0.04
  # wheel contact parameters
  wheel_min_depth = 0.015
  wheel_kp = "1e7"
  # ensure understeer with more friction at rear
  wheel_friction_front = 0.9
  wheel_friction_rear = 1.1

  # steering kinematics
  caster_angle_deg = 5
  caster_angle = caster_angle_deg * Math::PI / 180.0
  # inertial properties
  masses = {
    :chassis => 1356.0,
    :steering_wheel => 1.0,
    :front_left_wheel => 11,
    :front_right_wheel => 11,
    :rear_left_wheel => 11,
    :rear_right_wheel => 11,
  }
  mass = masses.values.inject(0.0, :+)
  mg = mass * grav

  # chassis center of mass location
  pos_from_model = {
    :front_wheels => -1.41,
    :rear_wheels => 1.45,
  }
  wheelbase = pos_from_model[:front_wheels].abs + pos_from_model[:rear_wheels].abs
  weight_distribution = {
    :front => 0.6,
    :rear => 0.4,
  }
  pos_from_model[:chassis_cg] = wheelbase*(1 - weight_distribution[:rear]) - pos_from_model[:rear_wheels]
  track_half_width_front = 0.760
  track_half_width_rear = 0.786
  track_half_width = 0.5 * (track_half_width_front + track_half_width_rear)
  chassis_cg_height = 0.48
  static_stability_factor = track_half_width / chassis_cg_height

  # max speed at which torque can be applied at wheels
  max_speed_mph = 85
  max_speed = max_speed_mph / 2.23694

  # chassis aerodynamic drag
  # deceleration per speed squared [m/s^2 / (m/s)^2] = [s]
  aero_drag_accel_gain = 4.5e-4
  aero_drag_force_gain = mass * aero_drag_accel_gain

  # rolling resistance applied as joint friction at axles
  rolling_resistance_coefficient = 0.014
  rolling_resistance_force = mg * rolling_resistance_coefficient
  rolling_resistance_torque = rolling_resistance_force * wheel_radius
  rolling_resistance_torque_front = weight_distribution[:front] * rolling_resistance_torque
  rolling_resistance_torque_rear  = weight_distribution[:rear]  * rolling_resistance_torque

  # maximum braking friction
  max_braking_friction = 0.8
  max_braking_force = mg * max_braking_friction
  max_braking_torque = max_braking_force * wheel_radius
  max_braking_torque_front = 0.5 * weight_distribution[:front] * max_braking_torque
  max_braking_torque_rear  = 0.5 * weight_distribution[:rear]  * max_braking_torque

  # maximum positive torque
  max_throttle_accel_g = 0.4
  max_throttle_force = mg * max_throttle_accel_g
  max_throttle_torque = max_throttle_force * wheel_radius
  max_throttle_torque_front = 0.5 * weight_distribution[:front] * max_throttle_torque
  max_throttle_torque_rear  = 0.5 * weight_distribution[:rear]  * max_throttle_torque
%>
<sdf version="1.6">
  <model name="prius_hybrid">
    <plugin name='priusHybrid' filename='libPriusHybridPlugin.so'>
      <chassis>chassis</chassis>
      <front_left_wheel>prius_hybrid::front_left_combined_joint</front_left_wheel>
      <front_right_wheel>prius_hybrid::front_right_combined_joint</front_right_wheel>
      <front_left_wheel_steering>prius_hybrid::front_left_combined_joint</front_left_wheel_steering>
      <front_right_wheel_steering>prius_hybrid::front_right_combined_joint</front_right_wheel_steering>
      <back_left_wheel>prius_hybrid::rear_left_wheel_joint</back_left_wheel>
      <back_right_wheel>prius_hybrid::rear_right_wheel_joint</back_right_wheel>
      <steering_wheel>prius_hybrid::steering_joint</steering_wheel>
      <chassis_aero_force_gain><%= aero_drag_force_gain %></chassis_aero_force_gain>
      <front_torque><%= max_throttle_torque_front %></front_torque>
      <back_torque><%= max_throttle_torque_rear %></back_torque>
      <front_brake_torque><%= max_braking_torque_front %></front_brake_torque>
      <back_brake_torque><%= max_braking_torque_rear %></back_brake_torque>
      <max_speed><%= max_speed %></max_speed>
      <!-- 37 deg ~= 0.6458 rad -->
      <max_steer>0.6458</max_steer>
      <flwheel_steering_p_gain>1e4</flwheel_steering_p_gain>
      <frwheel_steering_p_gain>1e4</frwheel_steering_p_gain>
      <flwheel_steering_i_gain>0</flwheel_steering_i_gain>
      <frwheel_steering_i_gain>0</frwheel_steering_i_gain>
      <flwheel_steering_d_gain>3e2</flwheel_steering_d_gain>
      <frwheel_steering_d_gain>3e2</frwheel_steering_d_gain>
    </plugin>

    <pose>0 0 0.03 0 0 0</pose>
    <link name="chassis">
      <inertial>
        <mass><%= masses[:chassis] %></mass>
        <pose>0 <%= -pos_from_model[:chassis_cg] %> <%= chassis_cg_height %> 0 0 0</pose>
        <inertia>
          <ixx>2581.13354740</ixx>
          <ixy>0.0</ixy>
          <iyy>591.30846112</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>2681.95008628</izz>
        </inertia>
      </inertial>

      <visual name="chassis_visual">
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Hybrid</name>
              <center>false</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>

      <visual name="interior_visual">
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Hybrid_Interior</name>
              <center>false</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>

      <visual name="windows_visual">
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Hybrid_Windows</name>
              <center>false</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>

      <collision name="chassis">
        <pose>0.0 0.05 0.625 0 0 0</pose>
        <geometry>
          <box>
            <size>1.7526 2.1 0.95</size>
          </box>
        </geometry>
      </collision>

      <collision name="front_bumper">
        <pose>0.0 -2.0 0.458488 0.0 0 0</pose>
        <geometry>
          <box>
            <size>1.337282 0.48 0.566691</size>
          </box>
        </geometry>
      </collision>

      <collision name="hood">
        <pose>0.0 -1.900842 0.676305 0.341247 0 0</pose>
        <geometry>
          <box>
            <size>1.597968 0.493107 0.265468</size>
          </box>
        </geometry>
      </collision>

      <collision name="windshield">
        <pose>0.0 -0.875105 1.032268 0.335476 0 0</pose>
        <geometry>
          <box>
            <size>1.168381 1.654253 0.272347</size>
          </box>
        </geometry>
      </collision>

      <collision name="top_front">
        <pose>0.0 0.161236 1.386042 0.135030 0 0</pose>
        <geometry>
          <box>
            <size>1.279154 0.625988 0.171868</size>
          </box>
        </geometry>
      </collision>

      <collision name="top_rear">
        <pose>0.0 0.817696 1.360069 -0.068997 0 0</pose>
        <geometry>
          <box>
            <size>1.285130 0.771189 0.226557</size>
          </box>
        </geometry>
      </collision>

      <collision name="rear_window">
        <pose>0.0 1.640531 1.175126 -0.262017 0 0</pose>
        <geometry>
          <box>
            <size>1.267845 1.116344 0.244286</size>
          </box>
        </geometry>
      </collision>

      <collision name="trunk">
        <pose>0.0 1.637059 0.888180 0.0 0 0</pose>
        <geometry>
          <box>
            <size>1.788064 1.138988 0.482746</size>
          </box>
        </geometry>
      </collision>

      <collision name="back_bumper">
        <pose>0.0 2.054454 0.577870 0.0 0 0</pose>
        <geometry>
          <box>
            <size>1.781650 0.512093 0.581427</size>
          </box>
        </geometry>
      </collision>
    </link>

    <link name="steering_wheel">
      <pose>0.357734 -0.627868 0.988243 -1.302101 0 0</pose>
      <inertial>
        <mass><%= masses[:steering_wheel] %></mass>
        <inertia>
          <ixx>0.14583300</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.14583300</iyy>
          <iyz>0</iyz>
          <izz>0.12500000</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.178172</radius>
            <length>0.041845</length>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <ode>
              <min_depth>0.003</min_depth>
            </ode>
          </contact>
        </surface>
      </collision>
      <visual name="visual">
        <pose>0 0 0 1.302101 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Steering_Wheel</name>
              <center>true</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>
    </link>

    <link name="front_left_wheel">
      <pose><%= track_half_width_front %> <%= pos_from_model[:front_wheels] %> 0.3 0 0 0</pose>
      <inertial>
        <mass><%= masses[:front_left_wheel] %></mass>
        <inertia>
          <ixx>0.58631238</ixx>
          <ixy>0.0</ixy>
          <iyy>0.33552910</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.33552910</izz>
        </inertia>
      </inertial>

      <visual name="visual">
        <pose><%= wheel_visual_offset %> 0.0 0.0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Wheel_Front_Left_</name>
              <center>true</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>

      <collision name="collision">
        <pose>0.0 0.0 0.0 0 <%= Math::PI/2 %> 0</pose>
        <geometry>
          <sphere>
            <radius><%= wheel_radius %></radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu><%= wheel_friction_front %></mu>
              <mu2><%= wheel_friction_front %></mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth><%= wheel_min_depth %></min_depth>
              <kp><%= wheel_kp %></kp>
            </ode>
          </contact>
        </surface>

      </collision>
    </link>

    <link name="front_right_wheel">
      <pose><%= -track_half_width_front %> <%= pos_from_model[:front_wheels] %> 0.3 0 0 0</pose>
      <inertial>
        <mass><%= masses[:front_right_wheel] %></mass>
        <inertia>
          <ixx>0.58631238</ixx>
          <ixy>0.0</ixy>
          <iyy>0.33552910</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.33552910</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <pose><%= -wheel_visual_offset %> 0.0 0.0 0 0 0</pose>
        <pose>0 0 0.0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Wheel_Front_Right</name>
              <center>true</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>

      <collision name="collision">
        <pose>0.0 0.0 0.0 0 <%= Math::PI/2 %> 0</pose>
        <geometry>
          <sphere>
            <radius><%= wheel_radius %></radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu><%= wheel_friction_front %></mu>
              <mu2><%= wheel_friction_front %></mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth><%= wheel_min_depth %></min_depth>
              <kp><%= wheel_kp %></kp>
            </ode>
          </contact>
        </surface>

      </collision>
    </link>


    <link name="rear_left_wheel">
      <pose><%= track_half_width_rear %> <%= pos_from_model[:rear_wheels] %> 0.3 0 0 0</pose>
      <inertial>
        <mass><%= masses[:rear_right_wheel] %></mass>
        <inertia>
          <ixx>0.58631238</ixx>
          <ixy>0.0</ixy>
          <iyy>0.33552910</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.33552910</izz>
        </inertia>
      </inertial>

      <visual name="visual">
        <pose><%= wheel_visual_offset %> 0.0 0.0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Wheel_Front_Left_</name>
              <center>true</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>

      <collision name="collision">
        <pose>0.0 0.0 0.0 0 <%= Math::PI/2 %> 0</pose>
        <geometry>
          <sphere>
            <radius><%= wheel_radius %></radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu><%= wheel_friction_rear %></mu>
              <mu2><%= wheel_friction_rear %></mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth><%= wheel_min_depth %></min_depth>
              <kp><%= wheel_kp %></kp>
            </ode>
          </contact>
        </surface>

      </collision>
    </link>

    <link name="rear_right_wheel">
      <pose><%= -track_half_width_rear %> <%= pos_from_model[:rear_wheels] %> 0.3 0 0 0</pose>
      <inertial>
        <mass><%= masses[:rear_left_wheel] %></mass>
        <inertia>
          <ixx>0.58631238</ixx>
          <ixy>0.0</ixy>
          <iyy>0.33552910</iyy>
          <ixz>0.0</ixz>
          <iyz>0.0</iyz>
          <izz>0.33552910</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <pose><%= -wheel_visual_offset %> 0.0 0.0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://prius_hybrid/meshes/Hybrid.obj</uri>
            <scale>0.01 0.01 0.01</scale>
            <submesh>
              <name>Wheel_Front_Right</name>
              <center>true</center>
            </submesh>
          </mesh>
        </geometry>
      </visual>

      <collision name="collision">
        <pose>0.0 0.0 0.0 0 <%= Math::PI/2 %> 0</pose>
        <geometry>
          <sphere>
            <radius><%= wheel_radius %></radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu><%= wheel_friction_rear %></mu>
              <mu2><%= wheel_friction_rear %></mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
          <contact>
            <ode>
              <min_depth><%= wheel_min_depth %></min_depth>
              <kp><%= wheel_kp %></kp>
            </ode>
          </contact>
        </surface>
      </collision>
    </link>

    <joint type="universal" name="front_left_combined_joint">
      <pose>0 0 0 <%= -caster_angle %> 0 0</pose>
      <child>front_left_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <!-- +- 50 degrees -->
          <lower>-0.8727</lower>
          <upper>0.8727</upper>
        </limit>
      </axis>
      <axis2>
        <xyz>1 0 0</xyz>
        <dynamics>
          <friction><%= 0.5*rolling_resistance_torque_front %></friction>
        </dynamics>
      </axis2>
    </joint>

    <joint type="universal" name="front_right_combined_joint">
      <pose>0 0 0 <%= -caster_angle %> 0 0</pose>
      <child>front_right_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <!-- +- 50 degrees -->
          <lower>-0.8727</lower>
          <upper>0.8727</upper>
        </limit>
      </axis>
      <axis2>
        <xyz>1 0 0</xyz>
        <dynamics>
          <friction><%= 0.5*rolling_resistance_torque_front %></friction>
        </dynamics>
      </axis2>
    </joint>

    <joint type="revolute" name="rear_left_wheel_joint">
      <child>rear_left_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <dynamics>
          <friction><%= 0.5*rolling_resistance_torque_rear %></friction>
        </dynamics>
      </axis>
    </joint>
    <joint type="revolute" name="rear_right_wheel_joint">
      <child>rear_right_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <dynamics>
          <friction><%= 0.5*rolling_resistance_torque_rear %></friction>
        </dynamics>
      </axis>
    </joint>

    <joint name="steering_joint" type="revolute">
      <pose>-0.002 0 0 0 0 0</pose>
      <parent>chassis</parent>
      <child>steering_wheel</child>
      <axis>
        <!--<xyz>0 1 0.84365</xyz>-->
        <xyz>0 0.964095 0.265556</xyz>
        <limit>
          <lower>-3.14</lower>
          <upper>3.14</upper>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <cfm_damping>1</cfm_damping>
        </ode>
      </physics>
    </joint>

  </model>

</sdf>
